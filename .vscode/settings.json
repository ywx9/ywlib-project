{
  "files.exclude": {
    "ywlib/bin/*": true,
    "ywlib/inc/*": true,
    "ywlib/lib/*": true,
    "ywlib/std/*": true,
  },
  "search.exclude": {
    "ywlib/inc/*": false,
  },
  "files.associations": {
    "*": "cpp",
    "*.bat": "bat",
    "*.json": "json",
    "*.md": "markdown",
    "*.py": "python",
    ".gitignore": "yaml",
    ".clang-format": "yaml",
    "LICENSE": "plaintext"
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
  }
}
